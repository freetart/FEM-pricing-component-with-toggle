@import url("https://fonts.googleapis.com/css2?family=Montserrat:wght@400;700&display=swap");
*,
*::before,
*::after {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
html {
  font-size: 62.5%;
  scroll-behavior: smooth;
}
body {
  font-family: "Montserrat", sans-serif;
  display: grid;
  place-content: center;
  min-height: 100vh;
  min-width: 100vw;
  background-color: #f6f6fe;
  position: relative;
  overflow-x: hidden;
}
a {
  text-decoration: none;
}
li {
  list-style: none;
}
img {
  width: 100%;
  object-fit: cover;
}
button {
  cursor: pointer;
  font-family: inherit;
}
input {
  font-family: inherit;
}
@media (prefers-reduced-motion: reduce) {
  *,
*::before,
*::after {
    animation-delay: -1ms !important;
    animation-duration: 1ms !important;
    animation-iteration-count: 1 !important;
    background-attachment: initial !important;
    scroll-behavior: auto !important;
    transition-duration: 0s !important;
    transition-delay: 0s !important;
  }
}
.toggle {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 3rem;
}
.toggle__option {
  font-weight: 700;
  letter-spacing: 0.1rem;
  line-height: 1.3;
  color: #b3b5c6;
  font-size: 1.8rem;
}
.switch {
  position: relative;
  display: inline-block;
  width: 4.5rem;
  height: 2.55rem;
  transition: all 0.3s ease-in-out;
}
.switch:hover, .switch:focus {
  opacity: 0.8;
}
.switch__checkbox {
  opacity: 0;
  width: 0;
  height: 0;
}
.switch__toggle {
  position: absolute;
  cursor: pointer;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  border-radius: 2.55rem;
  background: linear-gradient(#a3a8f0, #696fdd);
  -webkit-transition: all 0.3s ease-in-out;
  transition: all 0.3s ease-in-out;
}
.switch__toggle:before {
  position: absolute;
  content: "";
  height: 1.95rem;
  width: 1.95rem;
  left: 0.3rem;
  bottom: 0.3rem;
  background-color: white;
  border-radius: 50%;
  -webkit-transition: all 0.3s ease-in-out;
  transition: all 0.3s ease-in-out;
}
.switch__checkbox:checked + .switch__toggle:before {
  -webkit-transform: translateX(1.95rem);
  -ms-transform: translateX(1.95rem);
  transform: translateX(1.95rem);
}
.card {
  color: black;
  background-color: white;
  padding: 2rem;
  text-align: center;
  position: relative;
  z-index: 2;
  box-shadow: 0.1rem 0.4rem 0.5rem rgba(0, 0, 0, 0.1);
  border-radius: 1rem;
}
.card--primary {
  background: linear-gradient(#a3a8f0, #696fdd);
  color: white;
  transform: scale(1.1);
  z-index: 5;
}
@media (max-width: 850px) {
  .card--primary {
    transform: scale(1);
  }
}
.card--primary .card__plan,
.card--primary .card__price,
.card--primary .card__item {
  color: #f6f6fe;
}
.card--primary .card__item {
  border-top: 0.1rem solid #f6f6fe;
}
.card__plan {
  font-weight: 700;
  letter-spacing: 0.1rem;
  line-height: 1.3;
  color: #494c5f;
  font-size: 2rem;
  padding: 1rem;
}
.card__price {
  font-weight: 700;
  letter-spacing: 0.1rem;
  line-height: 1.3;
  color: #494c5f;
  font-size: 5.5rem;
  padding: 2rem;
}
.card__item {
  font-weight: 700;
  letter-spacing: 0.1rem;
  line-height: 1.3;
  color: #494c5f;
  font-size: 1.4rem;
  border-top: 0.1rem solid #b3b5c6;
  padding: 2rem;
}
.btn {
  font-weight: 700;
  letter-spacing: 0.1rem;
  line-height: 1.3;
  display: inline-block;
  text-align: center;
  width: 100%;
  padding: 1.2rem 2rem;
  font-size: 1.3rem;
  border-radius: 1rem;
  text-transform: uppercase;
  transition: all 0.3s ease-in-out;
}
.btn--primary {
  background: #f6f6fe;
  color: #696fdd;
  border: 0.1rem solid #f6f6fe;
}
.btn--primary:hover, .btn--primary:focus {
  background: transparent;
  color: #f6f6fe;
}
.btn--secondary {
  background: linear-gradient(to right, #a3a8f0, #696fdd);
  color: white;
  border: 0.1rem solid #a3a8f0;
}
.btn--secondary:hover, .btn--secondary:focus {
  opacity: 0.8;
}
.bg-pattern {
  position: absolute;
}
.bg-pattern--top {
  top: 0;
  right: 0;
  width: 30%;
}
@media (max-width: 600px) {
  .bg-pattern--top {
    width: 50%;
  }
}
.bg-pattern--bottom {
  bottom: -10%;
  left: 0;
  width: 30%;
}
@media (max-width: 600px) {
  .bg-pattern--bottom {
    width: 75%;
  }
}
.header {
  padding: 5rem 2rem;
  position: relative;
  z-index: 5;
}
.header__title {
  font-weight: 700;
  letter-spacing: 0.1rem;
  line-height: 1.3;
  text-align: center;
  font-size: 4.5rem;
  color: #6d708d;
  margin-bottom: 2rem;
}
.cards {
  padding: 5rem 2rem;
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 1rem;
}
@media (max-width: 850px) {
  .cards {
    grid-template-columns: 1fr;
    gap: 5rem;
  }
}
.tag {
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  flex-wrap: wrap;
  padding: 2rem 2rem 1rem 2rem;
  margin-top: 5rem;
  position: relative;
  z-index: 5;
}
.tag__heading {
  font-size: 1.6rem;
  color: black;
}
.tag__link {
  color: #a3a8f0;
  outline: 0;
}
.tag__link:hover, .tag__link:focus {
  text-decoration: underline;
}
.tag__logo {
  width: 5rem;
}
